{
  "compilerOptions": {
    "target": "es6",
    "esModuleInterop": true,
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "outDir": "./utils/gdocs2md/dist",
    "rootDir": "./utils/gdocs2md/src",
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "skipLibCheck": true
  },
  "include": [
    "./utils/gdocs2md/src", "./utils/gdocs2md/scripts", "utils/gdocs2md/archive"  ],
  "exclude": [
    "./test",
    "./src/client",
  ],
}